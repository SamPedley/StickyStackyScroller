{
  "name": "sticky-stacky-scrollr",
  "version": "0.1.0",
  "description": "Stacks elements as you scroll down.",
  "main": "dist/",
  "author": "Sam Pedley <sam@sampedley.com> (http://sampedley.com)",
  "scripts": {
    "start": "budo example/index.js --live -- -t babelify",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "build:main": "babel --copy-files --out-dir dist src",
    "lint": "standard"
  },
  "keywords": [
    "scrollr"
  ],
  "author": "Sam Pedley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/SamPedley/StickyStackyScroller.git"
  },
  "bugs": {
    "url": "https://github.com/SamPedley/StickyStackyScroller/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "budo": "^9.2.2",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "standard": "^8.6.0",
    "watchify": "^3.3.1",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "jquery": "^3.1.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "standard": {
    "ignore": [
      "dist",
      "example"
    ]
  }
}
